        "checkInDate": {
            "title": "Check-in Date",
            "type": "string",
            "description": "Check-in date in YYYY-MM-DD format (e.g., 2025-05-19). This is the date you want to check in to the hotel.",
            "editor": "textfield",
            "prefill": "2025-05-19"
        },
        "days": {
            "title": "Length of Stay",
            "type": "integer",
            "description": "Number of days to stay at the hotel. The scraper will calculate the check-out date automatically.",
            "minimum": 1,
            "maximum": 30,
            "default": 1
        },
        "adults": {
            "title": "Number of Adults",
            "type": "integer",
            "description": "Number of adult guests for the booking. This affects pricing calculations.",
            "minimum": 1,
            "maximum": 10,
            "default": 1
        },
        "maxRequestRetries": {
            "title": "Max Request Retries",
            "type": "integer",
            "description": "Maximum number of retries for failed requests. Increase this if you experience network issues.",
            "minimum": 0,
            "maximum": 10,
            "default": 3
        },
        "requestTimeoutSecs": {
            "title": "Request Timeout (seconds)",
            "type": "integer",
            "description": "Timeout for each request in seconds. Increase this if requests are timing out.",
            "minimum": 10,
            "maximum": 300,
            "default": 60
        },
        "proxyGroups": {
            "title": "Proxy Groups",
            "type": "array",
            "description": "List of proxy groups to use for requests. RESIDENTIAL proxies are recommended for better success rates.",
            "items": {
                "type": "string",
                "enum": [
                    "RESIDENTIAL",
                    "DATACENTER",
                    "MOBILE",
                    "SERPSERP"
                ]
            },
            "default": ["RESIDENTIAL"]
        },
        "proxyCountryCode": {
            "title": "Proxy Country Code",
            "type": "string",
            "description": "Country code for proxy location (e.g., 'US', 'GB', 'JP'). Leave empty to use proxies from any location.",
            "pattern": "^[A-Z]{2}$"
        }
    },
